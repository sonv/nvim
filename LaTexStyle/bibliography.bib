@article{FernholzFernholzZipfLaw2020,
  title = {Zipf’s Law for Atlas Models},
  author = {Fernholz, Ricardo T. and Fernholz, Robert},
  date = {2020-12},
  journaltitle = {Journal of Applied Probability},
  shortjournal = {J. Appl. Probab.},
  volume = {57},
