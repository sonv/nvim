@article{BergPurcellPhysicsChemoreception1977,
  title = {Physics of Chemoreception},
  author = {Berg, H.C. and Purcell, E.M.},
  date = {1977-11},
  journaltitle = {Biophysical Journal},
  shortjournal = {Biophysical Journal},
  volume = {20},
  number = {2},
  pages = {193--219},
  issn = {00063495},
  doi = {10.1016/S0006-3495(77)85544-6},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0006349577855446},
  urldate = {2024-05-31},
  abstract = {Statistical fluctuations limit the precision with which a microorganism can, in a given time T, determine the concentration of a chemoattractant in the surrounding medium. The best a cell can do is to monitor continually the state of occupation of receptors distributed over its surface. For nearly optimum performance only a small fraction of the surface need be specifically adsorbing. The probability that a molecule that has collided with the cell will find a receptor is Ns/(Ns + wa), if N receptors, each with a binding site of radius s, are evenly distributed over a cell of radius a. There is ample room for many independent systems of specific receptors. The adsorption rate for molecules of moderate size cannot be significantly enhanced by motion of the cell or by stirring of the medium by the cell. The least fractional error attainable in the determination of a concentration c is approximately (ThaD)-/2, where D is the diffusion constant of the attractant. The number of specific receptors needed to attain such precision is about a/s. Data on bacteriophage adsorption, bacterial chemotaxis, and chemotaxis in a cellular slime mold are evaluated. The chemotactic sensitivity of Escherichia coli approaches that of the cell of optimum design.},
  langid = {english},
  file = {/Users/sonvan/Documents/References/Berg-Purcell_1977_Physics_of_chemoreception.pdf;/Users/sonvan/Documents/References/Berg-Purcell_1977_Physics_of_chemoreception2.pdf}
}

