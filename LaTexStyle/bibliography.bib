@article{MattinglyStuartHighamErgodicitySDEs,
  title = {Ergodicity for {{SDEs}} and Approximations: Locally {{Lipschitz}} Vector {\"Y}elds and Degenerate Noise},
  author = {Mattingly, J C and Stuart, A M and Higham, D J},
  abstract = {The ergodic properties of SDEs, and various time discretizations for SDEs, are studied. The ergodicity of SDEs is established by using techniques from the theory of Markov chains on general state spaces, such as that expounded by Meyn--Tweedie. Application of these Markov chain results leads to straightforward proofs of geometric ergodicity for a variety of SDEs, including problems with degenerate noise and for problems with locally Lipschitz vector {\"y}elds. Applications where this theory can be usefully applied include damped-driven Hamiltonian problems (the Langevin equation), the Lorenz equation with degenerate noise and gradient systems.},
