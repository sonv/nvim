  primaryclass = {math},
  publisher = {arXiv},
  url = {http://arxiv.org/abs/1907.09256},
  urldate = {2024-06-07},
  abstract = {Using Zvonkin's transform and the Poisson equation in Rd with a parameter, we prove the averaging principle for stochastic differential equations with timedependent Ho{\textasciidieresis}lder continuous coefficients. Sharp convergence rates with order ({$\alpha$} {$\wedge$} 1)/2 in the strong sense and ({$\alpha$}/2) {$\wedge$} 1 in the weak sense are obtained, considerably extending the existing results in the literature. Moreover, we prove that the convergence of the multi-scale system to the effective equation depends only on the regularity of the coefficients of the equation for the slow variable, and does not depend on the regularity of the coefficients of the equation for the fast component.},
  archiveprefix = {arxiv},
  langid = {english},
  keywords = {Mathematics - Probability},
